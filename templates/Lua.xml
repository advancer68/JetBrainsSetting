<templateSet group="Lua">
  <template name="closure" value="function($PARAMS$) $END$ end" description="function() end" toReformat="true" toShortenFQNames="true">
    <variable name="PARAMS" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="fcc" value="function c:$FN$($PARAMS$)&#10;$END$&#10;end" description="local function functionName() end" toReformat="true" toShortenFQNames="true">
    <variable name="FN" expression="SuggestFirstLuaVarName()" defaultValue="&quot;funcName&quot;" alwaysStopAt="true" />
    <variable name="PARAMS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="LUA_CODE" value="true" />
    </context>
  </template>
  <template name="fccs" value="function c.$FN$($PARAMS$)&#10;$END$&#10;end" description="local function functionName() end" toReformat="true" toShortenFQNames="true">
    <variable name="FN" expression="completeSmart()" defaultValue="&quot;funcName&quot;" alwaysStopAt="true" />
    <variable name="PARAMS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="LUA_CODE" value="true" />
    </context>
  </template>
  <template name="var" value="local $name$ = $value$" description="create local var" toReformat="false" toShortenFQNames="true">
    <variable name="name" expression="completeSmart()" defaultValue="v" alwaysStopAt="true" />
    <variable name="value" expression="completeSmart()" defaultValue="{}" alwaysStopAt="true" />
    <context>
      <option name="LUA_FUNCTION" value="true" />
    </context>
  </template>
  <template name="cls" value="require &quot;Common/define&quot;&#10;require '3rd.oo.class'&#10;&#10;---@class $clsname$&#10;local c = Class()&#10;$end$&#10;return c" description="local function functionName() end" toReformat="true" toShortenFQNames="true">
    <variable name="clsname" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="true" />
    <variable name="end" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="LUA_CODE" value="true" />
    </context>
  </template>
  <template name="rql" value="local $name$ = require '$fpath$'" description="" toReformat="true" toShortenFQNames="true">
    <variable name="name" expression="SuggestFirstLuaVarName()" defaultValue="c" alwaysStopAt="true" />
    <variable name="fpath" expression="fileName()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="LUA_CODE" value="true" />
      <option name="LUA_FUNCTION" value="false" />
      <option name="LUA_IF" value="false" />
    </context>
  </template>
  <template name="rq" value="require '$fpath$'" description="" toReformat="true" toShortenFQNames="true">
    <variable name="fpath" expression="fileName()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="LUA_CODE" value="true" />
      <option name="LUA_FUNCTION" value="false" />
      <option name="LUA_IF" value="false" />
    </context>
  </template>
  <template name="func" value="function $FN$($PARAMS$)&#10;$END$&#10;end" description="function functionName() end" toReformat="true" toShortenFQNames="true">
    <variable name="FN" expression="" defaultValue="&quot;funcName&quot;" alwaysStopAt="true" />
    <variable name="PARAMS" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="lfunc" value="local function $FN$($PARAMS$)&#10;$END$&#10;end" description="local function functionName() end" toReformat="true" toShortenFQNames="true">
    <variable name="FN" expression="" defaultValue="&quot;funcName&quot;" alwaysStopAt="true" />
    <variable name="PARAMS" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="for" value="for $VAR$ = 1, $EXPR$ do&#10;$END$&#10;end" description="for i = 1, 10 do end" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="&quot;i&quot;" alwaysStopAt="true" />
    <variable name="EXPR" expression="SuggestFirstLuaVarName()" defaultValue="&quot;10&quot;" alwaysStopAt="true" />
    <context />
  </template>
  <template name="fori" value="for $VAR1$, $VAR2$ in ipairs($EXPR$) do&#10;$END$&#10;end" description="for i, v in ipairs(expr) do end" toReformat="true" toShortenFQNames="true">
    <variable name="VAR1" expression="" defaultValue="&quot;i&quot;" alwaysStopAt="true" />
    <variable name="VAR2" expression="" defaultValue="&quot;v&quot;" alwaysStopAt="true" />
    <variable name="EXPR" expression="SuggestFirstLuaVarName()" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="forp" value="for $VAR1$, $VAR2$ in pairs($EXPR$) do&#10;$END$&#10;end" description="for k, v in pairs(expr) do end" toReformat="true" toShortenFQNames="true">
    <variable name="VAR1" expression="" defaultValue="&quot;i&quot;" alwaysStopAt="true" />
    <variable name="VAR2" expression="" defaultValue="&quot;v&quot;" alwaysStopAt="true" />
    <variable name="EXPR" expression="SuggestFirstLuaVarName()" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="if" value="if $VAR$ then&#10;$END$&#10;end" description="if expr then end" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="SuggestFirstLuaVarName()" defaultValue="&quot;true&quot;" alwaysStopAt="true" />
    <context />
  </template>
  <template name="elseif" value="elseif $VAR$ then&#10;$END$" description="elseif expr then" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="SuggestFirstLuaVarName()" defaultValue="&quot;true&quot;" alwaysStopAt="true" />
    <context />
  </template>
  <template name="ifelse" value="if $VAR$ then&#10;$END$&#10;else&#10;end" description="if expr then else end" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="SuggestFirstLuaVarName()" defaultValue="&quot;true&quot;" alwaysStopAt="true" />
    <context />
  </template>
  <template name="paramsig" value="$PARAMS$" description="param signature" toReformat="false" toShortenFQNames="true">
    <variable name="PARAMS" expression="LuaFunctionParamSignature()" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
</templateSet>