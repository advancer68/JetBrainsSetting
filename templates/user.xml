<templateSet group="user">
  <template name="iftp" value="if type($arg1$) == '$typy$' then&#10;&#9;&#9;event = tostring(event)&#10;&#9;end" description="if type equal one then" toReformat="true" toShortenFQNames="true">
    <variable name="arg1" expression="SuggestFirstLuaVarName()" defaultValue="arg1" alwaysStopAt="true" />
    <variable name="typy" expression="classNameComplete()" defaultValue="table" alwaysStopAt="true" />
    <context>
      <option name="LUA_FUNCTION" value="true" />
    </context>
  </template>
  <template name="anf" value="---@field $pub$ $nam$ $tp$" description="annotetion" toReformat="true" toShortenFQNames="true">
    <variable name="pub" expression="" defaultValue="public" alwaysStopAt="false" />
    <variable name="nam" expression="SuggestFirstLuaVarName()" defaultValue="name" alwaysStopAt="true" />
    <variable name="tp" expression="classNameComplete()" defaultValue="table" alwaysStopAt="true" />
    <context>
      <option name="LUA_CODE" value="true" />
      <option name="LUA_FUNCTION" value="false" />
      <option name="LUA_IF" value="false" />
    </context>
  </template>
  <template name="anc" value="---@class $tp$" description="annotetion" toReformat="true" toShortenFQNames="true">
    <variable name="tp" expression="classNameComplete()" defaultValue="table" alwaysStopAt="true" />
    <context>
      <option name="LUA_CODE" value="true" />
      <option name="LUA_FUNCTION" value="false" />
      <option name="LUA_IF" value="false" />
    </context>
  </template>
  <template name="ifnl" value="if $va$ == nil then&#10;&#9;&#9;loge('account is null')&#10;&#9;&#9;return&#10;end" description="if null log and return" toReformat="true" toShortenFQNames="true">
    <variable name="va" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="LUA_FUNCTION" value="true" />
    </context>
  </template>
  <template name="asn" value="assert($va$,'[$fl$ $fc$] &lt;$va$&gt; is null')" description="assert is null" toReformat="true" toShortenFQNames="true">
    <variable name="va" expression="SuggestFirstLuaVarName()" defaultValue="va" alwaysStopAt="true" />
    <variable name="fl" expression="fileName()" defaultValue="" alwaysStopAt="false" />
    <variable name="fc" expression="LuaCurrentFunctionName()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="LUA_FUNCTION" value="true" />
    </context>
  </template>
  <template name="mid" value="---@type $pbmid$&#10;local msg = mid.newPb(mid.$pbmid$Mid)--&#10;$END$&#10;self:SendPblua(msg)" description="create and send pblua" toReformat="true" toShortenFQNames="true">
    <variable name="pbmid" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="LUA_FUNCTION" value="true" />
    </context>
  </template>
</templateSet>