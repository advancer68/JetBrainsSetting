<templateSet group="user">
  <template name="iftp" value="if type($arg1$) == '$typy$' then&#10;&#9;&#9;event = tostring(event)&#10;&#9;end" description="if type equal one then" toReformat="true" toShortenFQNames="true">
    <variable name="arg1" expression="SuggestFirstLuaVarName()" defaultValue="arg1" alwaysStopAt="true" />
    <variable name="typy" expression="classNameComplete()" defaultValue="table" alwaysStopAt="true" />
    <context>
      <option name="LUA_FUNCTION" value="true" />
    </context>
  </template>
  <template name="anf" value="---@field $pub$ $nam$ $tp$" description="annotetion" toReformat="true" toShortenFQNames="true">
    <variable name="nam" expression="SuggestFirstLuaVarName()" defaultValue="name" alwaysStopAt="true" />
    <variable name="tp" expression="classNameComplete()" defaultValue="table" alwaysStopAt="true" />
    <variable name="pub" expression="" defaultValue="public" alwaysStopAt="true" />
    <context>
      <option name="LUA_CODE" value="true" />
      <option name="LUA_FUNCTION" value="false" />
      <option name="LUA_IF" value="false" />
    </context>
  </template>
  <template name="anc" value="---@class $tp$" description="annotetion" toReformat="true" toShortenFQNames="true">
    <variable name="tp" expression="classNameComplete()" defaultValue="table" alwaysStopAt="true" />
    <context>
      <option name="LUA_CODE" value="true" />
      <option name="LUA_FUNCTION" value="false" />
      <option name="LUA_IF" value="false" />
    </context>
  </template>
  <template name="ifnl" value="if $va$ == nil then&#10;&#9;&#9;loge('account is null')&#10;&#9;&#9;return&#10;end" description="if null log and return" toReformat="true" toShortenFQNames="true">
    <variable name="va" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="LUA_FUNCTION" value="true" />
    </context>
  </template>
  <template name="asn" value="assert($va$,'[$fl$ $fc$] &lt;$va$&gt; is null')&#10;$END$" description="assert is null" toReformat="true" toShortenFQNames="true">
    <variable name="va" expression="SuggestFirstLuaVarName()" defaultValue="va" alwaysStopAt="true" />
    <variable name="fl" expression="fileName()" defaultValue="" alwaysStopAt="false" />
    <variable name="fc" expression="LuaCurrentFunctionName()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="LUA_FUNCTION" value="true" />
    </context>
  </template>
  <template name="req" value="---@type $pbmid$&#10;local msg = mid.newPb(mid.$pbmid$Mid)--&#10;$END$&#10;self:SendPblua(msg)" description="create and send pblua" toReformat="true" toShortenFQNames="true">
    <variable name="pbmid" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="LUA_FUNCTION" value="true" />
    </context>
  </template>
  <template name="resp" value="---@param msg $msg$&#10;function c:On$msg$(msg)&#10;    --self:AddNetEventMsgListener(mid.$msg$Mid,self.On$msg$)&#10;    { mid.$msg$Mid, self.On$msg$},&#10;&#9;$END$&#10;end" description="resp pb message" toReformat="true" toShortenFQNames="true">
    <variable name="msg" expression="classNameComplete()" defaultValue="Res" alwaysStopAt="true" />
    <context>
      <option name="LUA_CODE" value="true" />
      <option name="LUA_FUNCTION" value="false" />
      <option name="LUA_IF" value="false" />
    </context>
  </template>
  <template name="sfd" value="self.$na$ = self:FindTransf('$na$')&#10;$END$" description="self find and set" toReformat="true" toShortenFQNames="true">
    <variable name="na" expression="SuggestFirstLuaVarName()" defaultValue="name" alwaysStopAt="true" />
    <context>
      <option name="LUA_FUNCTION" value="true" />
    </context>
  </template>
  <template name="sfdd" value="self.$na$ = self:FindTransf('$ps$')&#10;$END$" description="self find a path and set" toReformat="true" toShortenFQNames="true">
    <variable name="na" expression="SuggestFirstLuaVarName()" defaultValue="name" alwaysStopAt="true" />
    <variable name="ps" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="LUA_FUNCTION" value="true" />
    </context>
  </template>
  <template name="clc" value="---@param gobj &#10;function c:OnClick$msg$(gobj)&#10;    { self.panel.$msg$, self.OnClick$msg$ },&#10;&#9;$END$&#10;end" description="resp pb message" toReformat="true" toShortenFQNames="true">
    <variable name="msg" expression="classNameComplete()" defaultValue="Res" alwaysStopAt="true" />
    <context>
      <option name="LUA_CODE" value="true" />
      <option name="LUA_FUNCTION" value="false" />
      <option name="LUA_IF" value="false" />
    </context>
  </template>
  <template name="ast" value="assert(type($va$) == '$tp$','[$fl$ $fc$] &lt;$va$&gt; type must is $tp$,but is ' .. type($va$))&#10;$END$" description="assert is type" toReformat="true" toShortenFQNames="true">
    <variable name="va" expression="SuggestFirstLuaVarName()" defaultValue="va" alwaysStopAt="true" />
    <variable name="tp" expression="classNameComplete()" defaultValue="table" alwaysStopAt="true" />
    <variable name="fl" expression="fileName()" defaultValue="" alwaysStopAt="false" />
    <variable name="fc" expression="LuaCurrentFunctionName()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="LUA_FUNCTION" value="true" />
    </context>
  </template>
  <template name="fcset" value="---@param value $tp$&#10;function c:Set$fna$(value)&#10;&#9;self.$fna$ = value&#10;end" description="define a function set a field" toReformat="true" toShortenFQNames="true">
    <variable name="fna" expression="SuggestFirstLuaVarName()" defaultValue="" alwaysStopAt="true" />
    <variable name="tp" expression="classNameComplete()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="LUA_CODE" value="true" />
      <option name="LUA_FUNCTION" value="false" />
      <option name="LUA_IF" value="false" />
    </context>
  </template>
  <template name="dfnt" value="    $va$='$va$',--&#10;    " description="define a notification" toReformat="true" toShortenFQNames="true">
    <variable name="va" expression="SuggestFirstLuaVarName()" defaultValue="Default" alwaysStopAt="true" />
    <context>
      <option name="LUA_CODE" value="true" />
      <option name="LUA_FUNCTION" value="false" />
      <option name="LUA_IF" value="false" />
    </context>
  </template>
  <template name="csntf" value="elseif info.name == Notif.$va$ then" description="else if info.name == Notif. then" toReformat="true" toShortenFQNames="true">
    <variable name="va" expression="SuggestFirstLuaVarName()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="LUA_FUNCTION" value="true" />
      <option name="LUA_IF" value="true" />
    </context>
  </template>
</templateSet>